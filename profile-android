# Save as /data/.profile

export HOME=/data
cd ~
clear

export SPARTACUS=/data/data/com.spartacusrex.spartacuside/files/system/bin
export PATH="$PATH:$SPARTACUS"

alias ssh='ssh -i ~/.ssh/id_rsa'

export EHOME=/mnt/sdcard/emacs

export PATH="$PATH:/data/data/org.burbas.erlang/arm_erlang_R14B_rel/bin"

# Run install_gcc first
# binaries are named arm-eabi-gcc, arm-eabi-g++, ...
export PATH="$PATH:/data/data/com.spartacusrex.spartacuside/files/android-gcc-4.4.0/bin"
